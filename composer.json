	"description": "An easy-to-use PHP Class for laravel for using the Instagram's API.",
	"keywords": ["instagram", "api", "laravel", "oauth"],
	"homepage": "https://github.com/emmanueln-nike/instagram-laravel-api",
	"license": "GNU",
	"authors": [
		{
			"name": "Emmanuel Ikechukwu N",
			"email": "free2liveb4u@gmail.com",
			"homepage": ""
		}
	],
	"require": {
		"php": ">=5.4.0",
		"ext-curl": "*"
	},
	"require-dev": {
			 "phpunit/phpunit" : "~4.0"
	 },
	"autoload": {
		"psr-4": {
